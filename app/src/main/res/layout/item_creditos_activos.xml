<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/txtNumeroCredito"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtNombreDeudor"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txtNumeroCredito"
        app:layout_constraintStart_toStartOf="@+id/txtNumeroCredito"
        app:layout_constraintTop_toBottomOf="@+id/txtNumeroCredito"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtTelefonoDeudor"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txtNombreDeudor"
        app:layout_constraintStart_toStartOf="@+id/txtNombreDeudor"
        app:layout_constraintTop_toBottomOf="@+id/txtNombreDeudor"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtValorDeuda"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txtTelefonoDeudor"
        app:layout_constraintStart_toStartOf="@+id/txtTelefonoDeudor"
        app:layout_constraintTop_toBottomOf="@+id/txtTelefonoDeudor"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtFechaUltimoAbono"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txtValorDeuda"
        app:layout_constraintStart_toStartOf="@+id/txtValorDeuda"
        app:layout_constraintTop_toBottomOf="@+id/txtValorDeuda"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>